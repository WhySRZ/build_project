<?xml version="1.0" encoding="utf-8"?>
    <LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_marginRight="@dimen/margin20"
        android:layout_marginLeft="@dimen/margin20"
        android:layout_marginTop="@dimen/margin20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:background="@drawable/regist_white"
        android:orientation="vertical">

        <RelativeLayout
            android:id="@+id/rl_resident_address"
            android:layout_width="match_parent"
            android:paddingRight="15dp"
            android:paddingLeft="15dp"
            android:layout_height="50dp"
            >
            <TextView
                android:id="@+id/resident_address_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="常驻地址 :"
                android:textColor="@color/text_color5"
                android:textSize="16dp"
                android:layout_centerVertical="true"/>

            <TextView
                android:id="@+id/tv_resident_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="50dp"
                android:layout_centerVertical="true"
                android:layout_marginLeft="50dp"
                android:layout_toRightOf="@id/resident_address_title"
                android:text="广东 深圳"
                android:textSize="15dp"
                android:textColor="@color/text_color1"/>

            <ImageView
                android:layout_centerInParent="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/image_resident_address"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/register_selector"/>
        </RelativeLayout>
        <View
            android:layout_marginRight="@dimen/margin20"
            android:layout_marginLeft="@dimen/margin20"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/line_color3"
            />
        <RelativeLayout
            android:id="@+id/rl_emergency_contact"
            android:layout_width="match_parent"
            android:paddingRight="15dp"
            android:paddingLeft="15dp"
            android:layout_height="50dp"
            >
            <TextView
                android:id="@+id/tv_emergency_contact_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="紧急联系人:"
                android:textColor="@color/text_color5"
                android:textSize="16dp"
                android:layout_centerVertical="true"/>


            <com.ys.jsst.pmis.buildclass.ui.widegt.ClearEditText
                android:textColor="@color/text_color"
                android:id="@+id/tv_emergency_contact"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="50dp"
                android:layout_centerVertical="true"
                android:inputType="text"
                android:background="@null"
                android:padding="5dp"
                android:textSize="14sp"
                android:text="建国"
                android:layout_toRightOf="@id/tv_emergency_contact_title"
                android:textCursorDrawable="@drawable/et_color"
                />


        </RelativeLayout>
        <View
            android:layout_marginRight="@dimen/margin20"
            android:layout_marginLeft="@dimen/margin20"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/line_color3"
            />
        <RelativeLayout
            android:id="@+id/rl_num"
            android:layout_width="match_parent"
            android:paddingRight="15dp"
            android:paddingLeft="15dp"
            android:layout_height="50dp"
            >
            <TextView
                android:id="@+id/tv_emergency_contact_num_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="联系电话   : "
                android:textColor="@color/text_color5"
                android:textSize="16dp"
                android:layout_centerVertical="true"/>


            <com.ys.jsst.pmis.buildclass.ui.widegt.ClearEditText
                android:textColor="@color/text_color"
                android:id="@+id/tv_number_emergency_contact"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="50dp"
                android:layout_centerVertical="true"
                android:inputType="number"
                android:background="@null"
                android:padding="5dp"
                android:textSize="14sp"
                android:text="12345678910"
                android:layout_toRightOf="@id/tv_emergency_contact_num_title"
                android:textCursorDrawable="@drawable/et_color"
                />


        </RelativeLayout>

    </LinearLayout>

