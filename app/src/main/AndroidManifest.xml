<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.ys.jsst.pmis.buildclass"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
<activity android:name=".login.LoginActivity"
          android:screenOrientation="portrait"
            android:theme="@style/AppTheme_Blue">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".login.ForgotPasswordActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"/>

        <activity android:name=".login.RemakePasswordActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"/>
        <activity android:name=".login.PasswordProtectActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"/>
        <activity android:name=".register.RegisterActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"/>
        <activity android:name=".register.ResumeActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"
                  />
        <activity android:name=".register.AddResumeActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"
            />
        <activity android:name=".register.ApplyAdministorActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"
            />

        <activity android:name=".register.CompanyRegisterActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"
            />

        <activity android:name=".home.MainActivity"
                  android:screenOrientation="portrait"
                  android:theme="@style/AppTheme_Blue"
            />
    </application>

</manifest>